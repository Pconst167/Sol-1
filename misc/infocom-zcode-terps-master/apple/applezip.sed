/^*/	{
	s/\*\(.*\)/;\1/p
	}

/^[^ 	*;]/ {
	s/\([^ 	]*\)[ 	]*\([^ 	]*\)[ 	]*\([^ 	]*\)[ 	]*\(.*\)/\1:	\2	\3	\4/p
	}

/^ /	{
	s/[ 	]*\([^ 	]*\)[ 	]*\([^ 	]*\)[ 	]*\(.*\)/	\1	\2	\3/p
	}

/=/	{
	s/^\([^	 :]*\)[	 :]*=\(.*\)/\1	EQU\2/p
	}

/</	{
	s/<\([ABCDEFGHIJKLMNOPQRSTUVWXYZ0123456789]*\)/HIGH \1/p
	}

/>/	{
	s/>\([ABCDEFGHIJKLMNOPQRSTUVWXYZ0123456789]*\)/LOW \1/p
	}

/PAG/	{
	s/PAG[^E]/PAGE/p
	}

/ASC/	{
	s/ASC[ 	]*/DB	/p
	}

/DFB/	{
	s/DFB[ 	]*/DB	/p
	}

/\*[+-]/	{
	s/\*+/$+/p
	s/\*-/$-/p
	}

